{
  "name": "movie-graphql",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.2",
    "node-fetch": "^2.1.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-relay": "^1.6.0",
    "react-scripts": "1.1.4",
    "relay-runtime": "^1.6.0"
  },
  "scripts": {
    "build": "node ./setup && react-scripts build",
    "test": "node ./setup && react-scripts test --env=jsdom",
    "start": "node ./setup && react-scripts start",
    "eject": "react-scripts eject",
    "relay": "get-graphql-schema http://localhost:4000/graphql > ./src/graphql.schema  && relay-compiler --src ./src --schema ./src/graphql.schema "
  },
  "proxy": "http://localhost:4000",
  "devDependencies": {
    "babel-plugin-relay": "^1.6.0",
    "relay-compiler": "^1.6.0"
  }
}
